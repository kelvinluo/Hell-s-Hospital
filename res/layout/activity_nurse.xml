<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/logincontainer"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.nurseapp.Nurse"
    tools:ignore="MergeRootFrame" >

    <Button
        android:id="@+id/getRecord"
        android:layout_width="140dp"
        android:layout_height="40dp"
        android:layout_gravity="left|bottom"
        android:layout_marginBottom="220dp"
        android:background="@color/Brown"
        android:gravity="left|center_vertical"
        android:paddingLeft="10dp"
        android:text="Patient Records"
        android:textColor="@color/White"
        android:textSize="15sp" />

    <Button
        android:id="@+id/rankedByUrgen"
        android:layout_width="280dp"
        android:layout_height="40dp"
        android:layout_gravity="left|bottom"
        android:layout_marginBottom="70dp"
        android:background="@color/Brown"
        android:gravity="left|center_vertical"
        android:paddingLeft="10dp"
        android:text="List of Patients (Ranked By Urgency)"
        android:textColor="@color/White"
        android:textSize="15sp" />

    <Button
        android:id="@+id/logoutBut"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:layout_gravity="left|bottom"
        android:layout_marginBottom="20dp"
        android:background="@color/Brown"
        android:gravity="left|center_vertical"
        android:minHeight="30dp"
        android:paddingLeft="10dp"
        android:text="Logout"
        android:textColor="@color/White"
        android:textSize="15dp" />

    <Button
        android:id="@+id/updatePatientRecord"
        android:layout_width="230dp"
        android:layout_height="40dp"
        android:layout_gravity="left|bottom"
        android:layout_marginBottom="120dp"
        android:layout_marginRight="0dp"
        android:background="@color/Brown"
        android:gravity="left|center_vertical"
        android:paddingLeft="10dp"
        android:text="Update Patient Record"
        android:textColor="@color/White"
        android:textSize="15sp" />

    <Button
        android:id="@+id/add_patient"
        android:layout_width="180dp"
        android:layout_height="40dp"
        android:layout_gravity="left|bottom"
        android:layout_marginBottom="170dp"
        android:background="@color/Brown"
        android:gravity="left|center_vertical"
        android:paddingLeft="10dp"
        android:text="New Patient Record"
        android:textColor="@color/White"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/userProfession"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="50dp"
        android:text="Welcome"
        android:textColor="@color/Brown" />

    <TextView
        android:id="@+id/userName"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:layout_gravity="left|top"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="45dp"
        android:gravity="center_vertical"
        android:text="TextView"
        android:textColor="@color/Brown"
        android:textSize="50sp" />

    <ImageView
        android:id="@+id/position_icon"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_gravity="left"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="35dp"
        android:src="@drawable/nurse" />

    <DigitalClock
        android:id="@+id/digitalClock1"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_gravity="bottom|left"
        android:layout_marginBottom="275dp"
        android:layout_marginLeft="5dp"
        android:gravity="center_vertical"
        android:text="DigitalClock"
        android:textColor="@color/Brown"
        android:textSize="30sp" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="fill_parent"
        android:layout_height="5dp"
        android:src="@color/Brown" />

    <ImageView
        android:id="@+id/ImageView01"
        android:layout_width="100dp"
        android:layout_height="2dp"
        android:layout_gravity="bottom|left"
        android:layout_marginBottom="270dp"
        android:layout_marginTop="140dp"
        android:src="@color/Brown" />

    <ImageView
        android:id="@+id/ImageView02"
        android:layout_width="fill_parent"
        android:layout_height="5dp"
        android:layout_gravity="bottom"
        android:src="@color/Brown" />

</FrameLayout>

